<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        body {
            background-color: aliceblue;
            padding: 27px;
            font-size: 21px;
            text-align: center;
        }
        li {
            margin: auto;
            width: fit-content;
        }
    </style>
</head>
<body>
    <h1>Profile: <%= person %> </h1>
    <h1>Age: <%= data.age %> </h1>
    <h1>Job: <%= data.job %> </h1>
    <h2>Friends:</h2>
    <ul>
        <% data.friends.forEach((friend) => { %>
            <li><%= friend %></li>
        <% }) %>
    </ul>
</body>
</html>